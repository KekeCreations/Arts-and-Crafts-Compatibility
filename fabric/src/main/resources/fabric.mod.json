{
    "schemaVersion": 1,
    "id": "arts_and_crafts_compat",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "KekeCreations",
        "Arbeet"
    ],
    "contact": {
        "homepage": "https://kekecreations.boomla.net/minecraft/java-mods/arts-and-crafts",
        "sources": "https://github.com/KekeCreations/Arts-and-Crafts/issues"
    },
    "license": "${license}",
    "icon": "${mod_id}.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.kekecreations.arts_and_crafts_compatibility.FabricArtsAndCraftsCompatibility"
        ],
        "client": [
            "com.kekecreations.arts_and_crafts_compatibility.client.FabricArtsAndCraftsCompatibilityClient"
        ],
        "fabric-datagen": [
            "com.kekecreations.arts_and_crafts_compatibility.datagen.ACCDataGenerator"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.14",
        "fabric": "*",
        "arts_and_crafts": ">=1.1.7",
        "minecraft": "${minecraft_version}",
        "java": ">=17"
    }
}
  